<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soyokra.sprival.dao.master.mapper.TradeMapper">
    <select id="selectOneTradeByTradeId"
            resultType="com.soyokra.sprival.dao.master.model.Trade"
    >
        select * from sprival.trade where trade_id = #{tradeId}
    </select>
</mapper>
